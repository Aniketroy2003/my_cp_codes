# cook your dish here
for i in range(int(input())):
    
    n = int(input())
    s = input()
    for j in range(0,n,2):
        if s[j]=="0" and s[j+1]=="0":
            print("A",end='')
        elif s[j]=="1" and s[j+1]=="0":
            print("C",end='')
        elif s[j]=="1" and s[j+1]=="1":
            print("G",end='')
        else:
            print("T",end='')
            
    print()
        # '00' = A
        # '01'=T
        # '11'=G
        # '10'=C
